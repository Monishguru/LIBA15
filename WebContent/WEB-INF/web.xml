<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>HostelManagement</display-name>
	<welcome-file-list>
		<welcome-file>admin.jsp</welcome-file>
	</welcome-file-list>


	<servlet>
		<servlet-name>AddAdminServlet</servlet-name>
		<servlet-class>com.hostelmanagement.controller.AddAdmin</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>AddAdminServlet</servlet-name>
		<url-pattern>/adminsignup</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>AddAdminServlet</servlet-name>
		<url-pattern>/adminlogin</url-pattern>
	</servlet-mapping>


</web-app>